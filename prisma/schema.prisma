model User {
  id     Int     @id @default(autoincrement())
  email  String  @unique
  name   String?
  routes Route[]
}

model Route {
  id             Int     @id @default(autoincrement())
  name           String
  grade          String
  description    String
  location       String
  protection     String
  routeCompleted Boolean @default(false)
  climber        User    @relation(fields: [authorId], references: [id])
  authorId       Int
}
