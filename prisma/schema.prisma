datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
  gear     Gear[]
}

model Gear {
  id         Int      @id @default(autoincrement())
  item       String
  dateBought DateTime
  climber    User     @relation(fields: [climberId], references: [id])
  climberId  Int      @default(autoincrement())
}
